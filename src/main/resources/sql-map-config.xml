<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="com.shop.biz.ProductVO" alias="product"></typeAlias>
		<typeAlias type="com.shop.biz.UserMember.UserMemberVO" alias="user"></typeAlias>
		<typeAlias type="com.shop.biz.Cart.CartVO" alias="cart"></typeAlias>
		<typeAlias type="com.shop.biz.Address.AddressVO" alias="address"></typeAlias>
		<typeAlias type="com.shop.biz.Order.OrderVO" alias="order"></typeAlias>
		<typeAlias type="com.shop.biz.Qna.QnaVO" alias="qna"></typeAlias>
		<typeAlias type="com.shop.biz.Admin.AdminVO" alias="admin"></typeAlias>
	</typeAliases>


	<mappers>
		<mapper resource="mappings/pro-mapping.xml"/>
		<mapper resource="mappings/user-mapping.xml"/>
		<mapper resource="mappings/cart-mapping.xml"/>
		<mapper resource="mappings/address-mapping.xml"/>
		<mapper resource="mappings/order-mapping.xml"/>
		<mapper resource="mappings/qna-mapping.xml"/>
		<mapper resource="mappings/admin-mapping.xml"/>
	</mappers>
</configuration>
